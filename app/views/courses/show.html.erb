<h1>Course: <%= @course.title %> </h1>
<h3>Students:</h3>

<ol>
  <% @course.students.each do |student| %>
    <li><a class ="student_name" href="../students/<%= student.id %>"><%= student.name %></a><span class="student_email"><%= student.email %></span></li>
  <% end %>
</ol>

<a href="/courses">Back to courses</a>
